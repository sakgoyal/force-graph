{
  "name": "force-graph-2",
  "version": "2.0.1",
  "description": "2D force-directed graph rendered on HTML5 canvas",
  "type": "module",
  "unpkg": "dist/index.umd.cjs",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "src/index.d.ts",
  "exports": {
    "types": "./dist/index.d.ts",
    "umd": "./dist/index.umd.cjs",
    "default": "./dist/index.js"
  },
  "sideEffects": [
    "./src/*.css"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sakgoyal/force-graph.git"
  },
  "homepage": "https://github.com/sakgoyal/force-graph",
  "keywords": [
    "2d",
    "force",
    "simulation",
    "graph",
    "canvas",
    "d3"
  ],
  "author": {
    "name": "Saksham Goyal",
    "url": "https://github.com/sakgoyal"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sakgoyal/force-graph/issues"
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts --minify",
    "lint": "tsc",
    "prepare": "npm run build"
  },
  "files": [
    "dist/**/*",
    "example/**/*",
    "src/**/*"
  ],
  "dependencies": {
    "@tweenjs/tween.js": "^23",
    "accessor-fn-2": "^2.0.3",
    "bezier-js": "^6",
    "canvas-color-tracker-2": "^2",
    "d3": "^7.9.0",
    "d3-force-3d": "^3.0.5",
    "index-array-by": "^1",
    "kapsule-2": "^2",
    "lodash-es": "^4"
  },
  "devDependencies": {
    "@types/bezier-js": "^4.1.3",
    "@types/d3": "^7.4",
    "@types/lodash-es": "^4.17",
    "@types/node": "^20",
    "tsup": "^8.2.2",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=20"
  }
}
